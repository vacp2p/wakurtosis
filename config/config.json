{
  "general": {
    "prng_seed": 1
  },
  "kurtosis": {
    "enclave_name": "wakurtosis",
    "topology_path": "./config/topology_generated/",
    "jobs": 4,
    "interconnect_nodes": true,
    "interconnection_batch": 10,
    "collectnet": {
      "protocol" : "relay",
      "version" : "2.0.0",
      "sampling_interval" : 1
    },
    "monitoring": true,
    "injection": true,
    "testing": false,
    "assertions": {
      "nomos": {
        "waiting": 60,
        "nodes_to_check": 1,
        "endpoint": "/carnot/info",
        "jq_extract": ".current_view",
        "expected_value": 10.0
      }
    }
  },
  "gennet": {
    "num_nodes": 100,
    "fanout": 20,
    "num_topics": 1,
    "num_partitions": 1,
    "num_subnets": 1,
    "container_size": "1",
    "node_type_distribution": {
      "nwaku:relay:rpc:metrics": 100,
      "gowaku:rln:dnsdisc:dns": 0,
      "nomos": 0
    },
    "network_type": "newmanwattsstrogatz",
    "output_dir": "network_data",
    "benchmark": "False"
  },
  "wls": {
    "debug_level": "DEBUG",
    "simulation_time": 60,
    "message_rate": 10,
    "min_packet_size": 2,
    "max_packet_size": 1024,
    "inter_msg_type": "poisson",
    "dist_type": "gaussian",
    "emitters_fraction": 1.0
  },
  "monitoring": {
    "wsl_pattern": "wls",
    "container_str_pattern": "waku",
    "process_str_pattern": "waku",
    "sampling_interval_s": 1,
    "probe_filename": "./probe.sh",
    "metrics_filename": "./cproc_metrics.json"
  },
  "plotting": {
    "by_node": [
      "container_cpu_load_average_10s",
      "container_memory_usage_bytes",
      "container_network_receive_bytes_total",
      "container_network_transmit_bytes_total",
      "container_fs_reads_bytes_total",
      "container_fs_writes_bytes_total"
    ]
  }
}
