          "config": "configuration_2"
        }
      },
      "configuration_1": {
        "subnetworks": ["A", "B"],
        "packet_loss": 0.1
      },
      "configuration_2": {
        "subnetworks": ["A", "B"],
        "packet_loss": 0.5,
        "packet_delay": {
          "type": "uniform"
        }
      }
    }
  },
  "gennet": {
    "num_nodes": 9,
