{
  "general":{
    "prng_seed" : 67
  },
  "kurtosis": {
    "enclave_name": "wakurtosis",
    "topology_path": "./config/topology_generated/",
    "jobs": 4,
    "interconnection_batch": 10
  },
  "gennet": {
    "num_nodes": 2500,
    "fanout": 3,
    "num_topics": 1,
    "num_partitions": 1,
    "num_subnets": 1,
    "container_size": "10",
    "node_type_distribution": { "nwaku":100, "gowaku":0 },
    "node_config": {
      "nwaku": "rpc-admin = true\nkeep-alive = true\nmetrics-server = true\ndiscv5-discovery = true\n",
      "gowaku": "rpc-admin = true\nmetrics-server = true\nrpc = true\n"
    },
    "network_type": "newmanwattsstrogatz",
    "output_dir": "network_data",
    "benchmark": "False"
  },
  "wls": {
    "debug_level": "DEBUG",
    "simulation_time": 6000,
    "message_rate": 100,
    "min_packet_size": 2,
    "max_packet_size": 1024,
    "inter_msg_type": "poisson",
    "dist_type": "gaussian",
    "emitters_fraction": 1.0
  },
  "monitoring": {
    "wsl_pattern": "wls",
    "container_str_pattern": "waku",
    "process_str_pattern": "waku",
    "sampling_interval_s": 1,
    "probe_filename" : "./monitoring/probe_fast_3.sh",
    "metrics_filename" : "./monitoring/metrics.json"
   }
}
