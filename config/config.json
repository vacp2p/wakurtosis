{
  "general":{
    "prng_seed" : 1
  },
  "kurtosis": {
    "enclave_name": "wakurtosis",
    "topology_path": "./config/topology_generated/",
    "jobs": 4,
    "interconnection_batch": 10
  },
  "gennet": {
    "num_nodes": 3,
    "fanout": 3,
    "num_topics": 1,
    "num_partitions": 1,
    "num_subnets": 1,
    "container_size": "1",
    "node_type_distribution": { "nwaku:relay:rpc:metrics":100, "gowaku:rln:dnsdisc:dns":0 },
    "network_type": "newmanwattsstrogatz",
    "output_dir": "network_data",
    "benchmark": "False"
  },
  "wls": {
    "debug_level": "DEBUG",
    "simulation_time": 60,
    "message_rate": 10,
    "min_packet_size": 2,
    "max_packet_size": 1024,
    "inter_msg_type": "poisson",
    "dist_type": "gaussian",
    "emitters_fraction": 1.0
  },
  "monitoring": {
    "wsl_pattern": "wls",
    "container_str_pattern": "waku",
    "process_str_pattern": "waku",
    "sampling_interval_s": 1,
    "probe_filename" : "./monitoring/probe.sh",
    "metrics_filename" : "./monitoring/metrics.json"
   }
}
